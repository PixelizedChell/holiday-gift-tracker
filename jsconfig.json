{
  "compilerOptions": {
    "target": "es2022",
    "module": "es2022",
    "jsx": "react-jsx",
    "checkJs": true,
    "baseUrl": "./",
    "ignoreDeprecations": "6.0",
    "paths": {
      "@components/*": [
        "components/*"
      ],
      "@app/*": [
        "app/*"
      ],
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "./*"
  ]
}